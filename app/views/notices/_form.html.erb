<%= form_with(model: [ @group, @notice ], local: true) do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :body, "Contact" %>
    <%= f.text_area :body, class: 'form-control' %>
  </div>
  
  <div class="form-group">
    <%= f.submit '送信', class: 'btn btn-success' %>
  </div>
  
<% end %>
